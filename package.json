{
  "name": "@ouellettec/design-system",
  "version": "1.2.0-alpha.4",
  "author": "Christopher Ouellette <chrisryanouellette@gmail.com>",
  "license": "MIT",
  "main": "build/index.js",
  "types": "./build/index.d.ts",
  "files": [
    "./build",
    "./index.d.ts",
    "./index.js",
    "./lib.d.ts",
    "./lib.js",
    "./ssr.d.ts",
    "./ssr.js",
    "./utils.d.ts",
    "./utils.js"
  ],
  "scripts": {
    "prepare": "husky install",
    "develop": "vite",
    "build": "yarn build:package && yarn build:format",
    "build:package": "vite build",
    "build:format": "prettier --loglevel warn --write \"build/**/*.d.ts\"",
    "preview": "vite preview",
    "emulate": "firebase emulators:start",
    "ci": "yarn lint",
    "pre-commit": "yarn lint",
    "test": "react-scripts test --watchAll=false",
    "test:watch": "react-scripts test",
    "test:coverage": "yarn test --coverage",
    "test:coverage:view": "yarn test:coverage --watchAll=false && yarn opener ./coverage/lcov-report/index.html",
    "test:debug": "react-scripts --inspect-brk test --runInBand",
    "lint": "yarn tsc && yarn eslint",
    "tsc": "tsc --noemit",
    "eslint": "yarn eslint:base .",
    "eslint:base": "eslint --ignore-path .gitignore --ext ts,tsx -c ./.eslintrc.js",
    "eslint:check": "yarn eslint --fix-dry-run .",
    "eslint:fix": "yarn eslint --fix .",
    "format": "prettier --ignore-path='./.gitignore' './**'",
    "format:fix": "prettier --ignore-path='./.gitignore' --write './**'",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "nookies": "^2.5.2",
    "react": "^18.2.0",
    "react-router": "^6.4.3",
    "react-router-dom": "^6.4.3",
    "react-transition-group": "^4.4.5",
    "web-vitals": "^3.0.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@storybook/addon-a11y": "^7.0.2",
    "@storybook/addon-actions": "^7.0.2",
    "@storybook/addon-essentials": "^7.0.2",
    "@storybook/addon-interactions": "^7.0.2",
    "@storybook/addon-links": "^7.0.2",
    "@storybook/node-logger": "^7.0.2",
    "@storybook/preset-create-react-app": "^7.0.2",
    "@storybook/react": "^7.0.2",
    "@storybook/react-vite": "^7.0.2",
    "@storybook/testing-library": "^0.1.0",
    "@testing-library/dom": "^8.19.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.2.2",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "@types/react-transition-group": "^4.4.5",
    "@typescript-eslint/parser": "^5.42.0",
    "@vitejs/plugin-react": "^2.2.0",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.26.0",
    "eslint-config-omlette": "1.0.4",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-storybook": "^0.6.11",
    "firebase": "^9.17.1",
    "firebase-admin": "^11.5.0",
    "husky": "^8.0.3",
    "jest": "^29.2.2",
    "msw": "^0.47.4",
    "next": "^13.1.6",
    "opener": "^1.5.2",
    "postcss": "^8.4.18",
    "prettier": "^2.8.7",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1",
    "remark-gfm": "^3.0.1",
    "storybook": "^7.0.2",
    "tailwindcss": "^3.2.1",
    "typescript": "^5.0.3",
    "vite": "^3.2.2",
    "vite-plugin-dts": "^2.2.0",
    "webpack-bundle-analyzer": "^4.7.0"
  },
  "peerDependencies": {
    "firebase": "9.17.1",
    "firebase-admin": "11.5.0",
    "next": "^13.1.6"
  },
  "volta": {
    "node": "16.19.1"
  }
}
